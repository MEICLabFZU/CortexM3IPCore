N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                               ALLOCATE AND FREE A TIMER
N*
N* Description: This function is called to allocate a timer.
N*
N* Arguments  : none
N*
N* Returns    : a pointer to a timer if one is available
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  OS_TMR  *OSTmr_Alloc (void)
S{
S    OS_TMR *ptmr;
S
S
S    if (OSTmrFreeList == (OS_TMR *)0) {
S        return ((OS_TMR *)0);
S    }
S    ptmr            = (OS_TMR *)OSTmrFreeList;
S    OSTmrFreeList   = (OS_TMR *)ptmr->OSTmrNext;
S    ptmr->OSTmrNext = (OS_TCB *)0;
S    ptmr->OSTmrPrev = (OS_TCB *)0;
S    OSTmrUsed++;
S    OSTmrFree--;
S    return (ptmr);
S}
N#endif
N
N
N/*
N************************************************************************************************************************
N*                                             RETURN A TIMER TO THE FREE LIST
N*
N* Description: This function is called to return a timer object to the free list of timers.
N*
N* Arguments  : ptmr     is a pointer to the timer to free
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  void  OSTmr_Free (OS_TMR *ptmr)
S{
S    ptmr->OSTmrState       = OS_TMR_STATE_UNUSED;      /* Clear timer object fields                                   */
S    ptmr->OSTmrOpt         = OS_TMR_OPT_NONE;
S    ptmr->OSTmrPeriod      = 0;
S    ptmr->OSTmrMatch       = 0;
S    ptmr->OSTmrCallback    = (OS_TMR_CALLBACK)0;
S    ptmr->OSTmrCallbackArg = (void *)0;
S#if OS_TMR_CFG_NAME_SIZE > 1
S    ptmr->OSTmrName[0]     = '?';                      /* Unknown name                                                */
S    ptmr->OSTmrName[1]     = OS_ASCII_NUL;
S#endif
S
S    ptmr->OSTmrPrev        = (OS_TCB *)0;              /* Chain timer to free list                                    */
S    ptmr->OSTmrNext        = OSTmrFreeList;
S    OSTmrFreeList          = ptmr;
S
S    OSTmrUsed--;                                       /* Update timer object statistics                              */
S    OSTmrFree++;
S}
N#endif
N
N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                                    INITIALIZATION
N*                                          INITIALIZE THE FREE LIST OF TIMERS
N*
N* Description: This function is called by OSInit() to initialize the free list of OS_TMRs.
N*
N* Arguments  : none
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Svoid  OSTmr_Init (void)
S{
S#if OS_EVENT_NAME_SIZE > 10
S    INT8U    err;
S#endif
S    INT16U   i;
S    OS_TMR  *ptmr1;
S    OS_TMR  *ptmr2;
S
S
S    OS_MemClr((INT8U *)&OSTmrTbl[0],      sizeof(OSTmrTbl));            /* Clear all the TMRs                         */
S    OS_MemClr((INT8U *)&OSTmrWheelTbl[0], sizeof(OSTmrWheelTbl));       /* Clear the timer wheel                      */
S
S    ptmr1 = &OSTmrTbl[0];
S    ptmr2 = &OSTmrTbl[1];
S    for (i = 0; i < (OS_TMR_CFG_MAX - 1); i++) {                        /* Init. list of free TMRs                    */
S        ptmr1->OSTmrType    = OS_TMR_TYPE;
S        ptmr1->OSTmrState   = OS_TMR_STATE_UNUSED;                      /* Indicate that timer is inactive            */
S        ptmr1->OSTmrNext    = (void *)ptmr2;                            /* Link to next timer                         */
S#if OS_TMR_CFG_NAME_SIZE > 1
S        ptmr1->OSTmrName[0] = '?';                                      /* Unknown name                               */
S        ptmr1->OSTmrName[1] = OS_ASCII_NUL;
S#endif
S        ptmr1++;
S        ptmr2++;
S    }
S    ptmr1->OSTmrType    = OS_TMR_TYPE;
S    ptmr1->OSTmrState   = OS_TMR_STATE_UNUSED;                          /* Indicate that timer is inactive            */
S    ptmr1->OSTmrNext    = (void *)0;                                    /* Last OS_TMR                                */
S#if OS_TMR_CFG_NAME_SIZE > 1
S    ptmr1->OSTmrName[0] = '?';                                          /* Unknown name                               */
S    ptmr1->OSTmrName[1] = OS_ASCII_NUL;
S#endif
S    OSTmrTime           = 0;
S    OSTmrUsed           = 0;
S    OSTmrFree           = OS_TMR_CFG_MAX;
S    OSTmrFreeList       = &OSTmrTbl[0];
S    OSTmrSem            = OSSemCreate(1);
S    OSTmrSemSignal      = OSSemCreate(0);
S
S#if OS_EVENT_NAME_SIZE > 18
S    OSEventNameSet(OSTmrSem,       (INT8U *)"uC/OS-II TmrLock",   &err);/* Assign names to semaphores                 */
S#else
S#if OS_EVENT_NAME_SIZE > 10
S    OSEventNameSet(OSTmrSem,       (INT8U *)"OS-TmrLock",         &err);
S#endif
S#endif
S
S#if OS_EVENT_NAME_SIZE > 18
S    OSEventNameSet(OSTmrSemSignal, (INT8U *)"uC/OS-II TmrSignal", &err);
S#else
S#if OS_EVENT_NAME_SIZE > 10
S    OSEventNameSet(OSTmrSemSignal, (INT8U *)"OS-TmrSig",          &err);
S#endif
S#endif
S
S    OSTmr_InitTask();
S}
N#endif
N
