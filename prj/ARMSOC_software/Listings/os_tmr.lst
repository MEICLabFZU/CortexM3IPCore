N*                               OS_ERR_TMR_INVALID_TYPE   'ptmr'  is not pointing to an OS_TMR
N*                               OS_ERR_TMR_ISR            if the call was made from an ISR
N*                               OS_ERR_TMR_INACTIVE       'ptmr' points to a timer that is not active
N*                               OS_ERR_TMR_INVALID_STATE  if the timer is not in a valid state
N*
N* Returns    : The current state of the timer (see description).
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
SINT8U  OSTmrStateGet (OS_TMR  *ptmr,
S                      INT8U   *perr)
S{
S    INT8U  state;
S
S
S#if OS_ARG_CHK_EN > 0
S    if (perr == (INT8U *)0) {
S        return (0);
S    }
S    if (ptmr == (OS_TMR *)0) {
S        *perr = OS_ERR_TMR_INVALID;
S        return (0);
S    }
S#endif
S    if (ptmr->OSTmrType != OS_TMR_TYPE) {              /* Validate timer structure                                    */
S        *perr = OS_ERR_TMR_INVALID_TYPE;
S        return (0);
S    }
S    if (OSIntNesting > 0) {                            /* See if trying to call from an ISR                           */
S        *perr = OS_ERR_TMR_ISR;
S        return (0);
S    }
S    OSTmr_Lock();
S    state = ptmr->OSTmrState;
S    switch (state) {
S        case OS_TMR_STATE_UNUSED:   
S        case OS_TMR_STATE_STOPPED:  
