S                       (void *)0,
S                       &OSTmrTaskStk[0],
S                       OS_TASK_TMR_PRIO);
S    #endif
S#endif
S
S#if OS_TASK_NAME_SIZE > 12
S    OSTaskNameSet(OS_TASK_TMR_PRIO, (INT8U *)"uC/OS-II Tmr", &err);
S#else
