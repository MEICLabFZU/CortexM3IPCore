S
S
S#if OS_TASK_CREATE_EXT_EN > 0
S    #if OS_STK_GROWTH == 1
S    (void)OSTaskCreateExt(OSTmr_Task,
S                          (void *)0,                                       /* No arguments passed to OSTmrTask()      */
S                          &OSTmrTaskStk[OS_TASK_TMR_STK_SIZE - 1],         /* Set Top-Of-Stack                        */
S                          OS_TASK_TMR_PRIO,
S                          OS_TASK_TMR_ID,
S                          &OSTmrTaskStk[0],                                /* Set Bottom-Of-Stack                     */
