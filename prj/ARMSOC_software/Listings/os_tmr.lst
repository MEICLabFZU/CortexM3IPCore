N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                         INSERT A TIMER INTO THE TIMER WHEEL
N*
N* Description: This function is called to insert the timer into the timer wheel.  The timer is always inserted at the
N*              beginning of the list.
N*
N* Arguments  : ptmr          Is a pointer to the timer to insert.
N*
N*              type          Is either:
N*                               OS_TMR_LINK_PERIODIC    Means to re-insert the timer after a period expired
N*                               OS_TMR_LINK_DLY         Means to insert    the timer the first time
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  void  OSTmr_Link (OS_TMR *ptmr, INT8U type)
S{
S    OS_TMR       *ptmr1;
S    OS_TMR_WHEEL *pspoke;
S    INT16U        spoke;
S
S
S    ptmr->OSTmrState = OS_TMR_STATE_RUNNING;
S    if (type == OS_TMR_LINK_PERIODIC) {                            /* Determine when timer will expire                */
S        ptmr->OSTmrMatch = ptmr->OSTmrPeriod + OSTmrTime;
S    } else {
S        if (ptmr->OSTmrDly == 0) {
S            ptmr->OSTmrMatch = ptmr->OSTmrPeriod + OSTmrTime;
S        } else {
S            ptmr->OSTmrMatch = ptmr->OSTmrDly    + OSTmrTime;
S        }
S    }
S    spoke  = (INT16U)(ptmr->OSTmrMatch % OS_TMR_CFG_WHEEL_SIZE);
S    pspoke = &OSTmrWheelTbl[spoke];
S
S    if (pspoke->OSTmrFirst == (OS_TMR *)0) {                       /* Link into timer wheel                           */
S        pspoke->OSTmrFirst   = ptmr;
S        ptmr->OSTmrNext      = (OS_TMR *)0;
S        pspoke->OSTmrEntries = 1;
S    } else {
S        ptmr1                = pspoke->OSTmrFirst;                 /* Point to first timer in the spoke               */
S        pspoke->OSTmrFirst   = ptmr;
S        ptmr->OSTmrNext      = (void *)ptmr1;
S        ptmr1->OSTmrPrev     = (void *)ptmr;
S        pspoke->OSTmrEntries++;
S    }
S    ptmr->OSTmrPrev = (void *)0;                                   /* Timer always inserted as first node in list     */
S}
N#endif
N
N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                         REMOVE A TIMER FROM THE TIMER WHEEL
N*
N* Description: This function is called to remove the timer from the timer wheel.
N*
N* Arguments  : ptmr          Is a pointer to the timer to remove.
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  void  OSTmr_Unlink (OS_TMR *ptmr)
S{
S    OS_TMR        *ptmr1;
S    OS_TMR        *ptmr2;
S    OS_TMR_WHEEL  *pspoke;
S    INT16U         spoke;
S
S
S    spoke  = (INT16U)(ptmr->OSTmrMatch % OS_TMR_CFG_WHEEL_SIZE);
S    pspoke = &OSTmrWheelTbl[spoke];
S
S    if (pspoke->OSTmrFirst == ptmr) {                       /* See if timer to remove is at the beginning of list     */
S        ptmr1              = (OS_TMR *)ptmr->OSTmrNext;
S        pspoke->OSTmrFirst = (OS_TMR *)ptmr1;
S        if (ptmr1 != (OS_TMR *)0) {
S            ptmr1->OSTmrPrev = (void *)0;
S        }
S    } else {
S        ptmr1            = (OS_TMR *)ptmr->OSTmrPrev;       /* Remove timer from somewhere in the list                */
S        ptmr2            = (OS_TMR *)ptmr->OSTmrNext;
S        ptmr1->OSTmrNext = ptmr2;
S        if (ptmr2 != (OS_TMR *)0) {
S            ptmr2->OSTmrPrev = (void *)ptmr1;
S        }
S    }
S    ptmr->OSTmrState = OS_TMR_STATE_STOPPED;
S    ptmr->OSTmrNext  = (void *)0;
S    ptmr->OSTmrPrev  = (void *)0;
S    pspoke->OSTmrEntries--;
S}
N#endif
N
N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                       TIMER MANAGER DATA STRUCTURE LOCKING MECHANISM
N*
N* Description: These functions are used to gain exclusive access to timer management data structures.
N*
N* Arguments  : none
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  void  OSTmr_Lock (void)
S{
S    INT8U  err;
S
S
S    OSSemPend(OSTmrSem, 0, &err);
S    (void)err;
S}
N#endif
N
N
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  void  OSTmr_Unlock (void)
S{
S    (void)OSSemPost(OSTmrSem);
S}
N#endif
N
N/*$PAGE*/
N/*
N************************************************************************************************************************
N*                                                 TIMER MANAGEMENT TASK
N*
N* Description: This task is created by OSTmrInit().
N*
N* Arguments  : none
N*
N* Returns    : none
N************************************************************************************************************************
N*/
N
N#if OS_TMR_EN > 0
X#if 0 > 0
Sstatic  void  OSTmr_Task (void *p_arg)
S{
S    INT8U            err;
S    OS_TMR          *ptmr;
S    OS_TMR          *ptmr_next;
S    OS_TMR_CALLBACK  pfnct;
S    OS_TMR_WHEEL    *pspoke;
S    INT16U           spoke;
S
S
S    (void)p_arg;                                                 /* Not using 'p_arg', prevent compiler warning       */
S    for (;;) {
S        OSSemPend(OSTmrSemSignal, 0, &err);                      /* Wait for signal indicating time to update timers  */
S        OSTmr_Lock();
S        OSTmrTime++;                                             /* Increment the current time                        */
S        spoke  = (INT16U)(OSTmrTime % OS_TMR_CFG_WHEEL_SIZE);    /* Position on current timer wheel entry             */
S        pspoke = &OSTmrWheelTbl[spoke];
S        ptmr   = pspoke->OSTmrFirst;
S        while (ptmr != (OS_TMR *)0) {
S            ptmr_next = (OS_TMR *)ptmr->OSTmrNext;               /* Point to next timer to update because current ... */
S                                                                 /* ... timer could get unlinked from the wheel.      */
S            if (OSTmrTime == ptmr->OSTmrMatch) {                 /* Process each timer that expires                   */
S                pfnct = ptmr->OSTmrCallback;                     /* Execute callback function if available            */
S                if (pfnct != (OS_TMR_CALLBACK)0) {
S                    (*pfnct)((void *)ptmr, ptmr->OSTmrCallbackArg);
S                }
S                OSTmr_Unlink(ptmr);                              /* Remove from current wheel spoke                   */
S                if (ptmr->OSTmrOpt == OS_TMR_OPT_PERIODIC) {
S                    OSTmr_Link(ptmr, OS_TMR_LINK_PERIODIC);      /* Recalculate new position of timer in wheel        */
S                } else {
S                    ptmr->OSTmrState = OS_TMR_STATE_COMPLETED;   /* Indicate that the timer has completed             */
S                }
S            }
S            ptmr = ptmr_next;
S        }
S        OSTmr_Unlock();
S    }
S}
N#endif
