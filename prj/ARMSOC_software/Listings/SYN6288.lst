N#define UART0_BASE     (APBPERIPH_BASE + 0x4000)
N#define UART1_BASE     (APBPERIPH_BASE + 0x5000)
N#define UART2_BASE     (APBPERIPH_BASE + 0x6000)
N//-------------------------------------
N// Programmer's model
N// 0x00 R     RXD[7:0]    Received Data
N//      W     TXD[7:0]    Transmit data
N// 0x04 RW    STAT[3:0]
N//              [3] RX buffer overrun (write 1 to clear)
N//              [2] TX buffer overrun (write 1 to clear)
N//              [1] RX buffer full (Read only)
N//              [0] TX buffer full (Read only)
N// 0x08 RW    CTRL[3:0]   TxIntEn, RxIntEn, TxEn, RxEn
N//              [6] High speed test mode Enable
N//              [5] RX overrun interrupt enable
N//              [4] TX overrun interrupt enable
N//              [3] RX Interrupt Enable
N//              [2] TX Interrupt Enable
N//              [1] RX Enable
N//              [0] TX Enable
N// 0x0C R/Wc  intr_status/INTCLEAR
N//              [3] RX overrun interrupt
N//              [2] TX overrun interrupt
N//              [1] RX interrupt
N//              [0] TX interrupt
N// 0x10 RW    BAUDDIV[19:0] Baud divider
N//            (minimum value is 16)
N// 0x3E0 - 0x3FC  ID registers
N//-------------------------------------
N//#define UART0_THR    		*(volatile unsigned *)(UART0_BASE+0x00)
N//#define UART0_RBR    		*(volatile unsigned *)(UART0_BASE+0x00)
N//#define UART0_STAT   		*(volatile unsigned *)(UART0_BASE+0x04)
N//#define UART0_CTRL    	*(volatile unsigned *)(UART0_BASE+0x08)
N//#define UART0_INTR    	*(volatile unsigned *)(UART0_BASE+0x0C)
N//#define UART0_BAUDDDIV  *(volatile unsigned *)(UART0_BASE+0x10)
N
N#ifdef __cplusplus
S  #define   __I     volatile             /*!< Defines 'read only' permissions                 */
N#else
N  #define   __I     volatile const       /*!< Defines 'read only' permissions                 */
N#endif
N#define     __O     volatile             /*!< Defines 'write only' permissions                */
N#define     __IO    volatile             /*!< Defines 'read / write' permissions              */
N
Ntypedef struct
N{
N  __IO   uint32_t  DATA;          /*!< Offset: 0x000 Data Register    (R/W) */
X  volatile   uint32_t  DATA;           
N  __IO   uint32_t  STATE;         /*!< Offset: 0x004 Status Register  (R/W) */
X  volatile   uint32_t  STATE;          
N  __IO   uint32_t  CTRL;          /*!< Offset: 0x008 Control Register (R/W) */
X  volatile   uint32_t  CTRL;           
N  union {
N    __I    uint32_t  INTSTATUS;   /*!< Offset: 0x00C Interrupt Status Register (R/ ) */
X    volatile const    uint32_t  INTSTATUS;    
N    __O    uint32_t  INTCLEAR;    /*!< Offset: 0x00C Interrupt Clear Register ( /W) */
X    volatile    uint32_t  INTCLEAR;     
N    };
N  __IO   uint32_t  BAUDDIV;       /*!< Offset: 0x010 Baudrate Divider Register (R/W) */
X  volatile   uint32_t  BAUDDIV;        
N
N} UART_TypeDef;
N
N#define UART0							((volatile UART_TypeDef   *) UART0_BASE   )
N#define UART1							((volatile UART_TypeDef   *) UART1_BASE   )
N#define UART2							((volatile UART_TypeDef   *) UART2_BASE   )
