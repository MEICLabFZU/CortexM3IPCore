N#if ((OS_EVENT_EN) && (OS_EVENT_MULTI_EN > 0))
X#if (((((0 > 0) && (4 > 0)) || (1 > 0) || (1 > 0) || (0 > 0))) && (1 > 0))
Nvoid  OS_EventTaskRemoveMulti (OS_TCB    *ptcb,
