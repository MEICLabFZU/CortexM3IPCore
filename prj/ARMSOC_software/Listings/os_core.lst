N    x    = OSUnMapTbl[pevent->OSEventTbl[y]];
N    prio = (INT8U)((y << 3) + x);                       /* Find priority of task getting the msg       */
N#else
S    if ((pevent->OSEventGrp & 0xFF) != 0) {             /* Find HPT waiting for message                */
S        y = OSUnMapTbl[ pevent->OSEventGrp & 0xFF];
S    } else {
S        y = OSUnMapTbl[(pevent->OSEventGrp >> 8) & 0xFF] + 8;
S    }
S    ptbl = &pevent->OSEventTbl[y];
S    if ((*ptbl & 0xFF) != 0) {
